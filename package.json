    "@typescript-eslint/parser": "7.0.2",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-typescript": "12.0.0",
    "bootstrap": "5.3.3",
    "bootstrap-vue-next": "0.16.6",
    "chrome-webstore-upload-cli": "3.1.0",
