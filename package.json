{
  "name": "bookmark-bar-switcher",
  "version": "1.4.0",
  "displayName": "Bookmark Bar Switcher",
  "author": "Daniel Purtov",
  "description": "Conveniently switch between multiple bookmark bars.",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=21.2.0",
    "npm": ">=10.2.4"
  },
  "scripts": {
    "build": "vue-tsc && vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "lint": "eslint --ext .ts ./src ./__tests__",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:build": "npm run build && vitest run",
    "release": "chrome-webstore-upload upload --source=dist --auto-publish"
  },
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "@fortawesome/free-solid-svg-icons": "6.5.1",
    "@fortawesome/vue-fontawesome": "3.0.5",
    "@tsconfig/node21": "21.0.0",
    "@types/bootstrap": "5.2.10",
    "@types/chrome": "0.0.253",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-vue": "4.5.1",
    "@vue/eslint-config-typescript": "12.0.0",
    "bootstrap": "5.3.2",
    "bootstrap-vue-next": "0.15.5",
    "chrome-webstore-upload-cli": "2.2.2",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "2.1.1",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-vitest": "0.3.10",
    "eslint-plugin-vue": "9.19.2",
    "jsdom": "^23.0.1",
    "prettier": "2.8.8",
    "puppeteer": "21.5.2",
    "sass": "1.69.5",
    "ts-node": "10.9.1",
    "typescript": "5.3.2",
    "unplugin-vue-components": "0.25.2",
    "vite": "5.0.4",
    "vite-plugin-web-extension": "4.0.0",
    "vitest": "^1.0.0-beta.6",
    "vue": "3.3.9",
    "vue-dndrop": "1.3.1",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "1.8.24"
  }
