{
  "name": "bookmark-bar-switcher",
  "version": "1.3.4",
  "displayName": "Bookmark Bar Switcher",
  "author": "Daniel Purtov",
  "description": "Conveniently switch between multiple bookmark bars.",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=21.1.0",
    "npm": ">=10.2.1"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "lint": "eslint --ext .ts ./src ./__tests__",
    "test": "jest",
    "test:build": "npm run build && jest",
    "release": "chrome-webstore-upload upload --source=dist --auto-publish"
  },
  "devDependencies": {
    "@faker-js/faker": "8.2.0",
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-solid-svg-icons": "6.4.2",
    "@fortawesome/vue-fontawesome": "3.0.5",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@samrum/vite-plugin-web-extension": "5.0.0",
    "@types/bootstrap": "5.2.8",
    "@types/chrome": "0.0.250",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.9.1",
    "@vitejs/plugin-vue": "4.4.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "bootstrap": "5.3.2",
    "chrome-webstore-upload-cli": "2.2.2",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "2.1.1",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-vue": "9.18.1",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "puppeteer": "21.5.0",
    "sass": "1.69.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vue": "3.3.8",
    "vue-dndrop": "1.3.1",
    "vue-tsc": "1.8.22"
